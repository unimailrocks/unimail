{
  "parser": "babel-eslint",
  "plugins": ["meteor"],
  "extends": ["airbnb", "plugin:meteor/recommended"],
  "rules": {
    "no-use-before-define": ["error", { "functions": false }],
    "react/jsx-filename-extension": 0,
    "arrow-parens": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/no-absolute-path": 0
  }
}
